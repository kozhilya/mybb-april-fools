{
  "ignorePatterns": [
    "node_modules/",
    "settings.*.js",
    "webpack.*.js"
  ],
  "plugins": ["@typescript-eslint"],
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended", "google"],
  "parser": "@babel/eslint-parser",
  "globals": {
    "window": true,
    "$": true,
    "jQuery": true
  },
  "env": {
    "browser": true
  },
  "rules": {
    "no-undef": "off",
    "getter-return": "off",
    "indent": "off",
    "no-unused-vars": "off",
    "new-cap": "off",
    "no-debugger": "off",
    "@typescript-eslint/no-unused-vars": "off"
  }
}